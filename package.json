{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "prettier": "prettier . --check",
    "test": "eslint . && prettier --list-different .",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@nuxtjs/google-fonts": "^3.2.0",
    "nuxt": "^3.13.0",
    "nuxt-swiper": "^1.2.2",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.5.7",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^4.3.8",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "sass": "^1.79.2",
    "typescript": "latest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue,scss}": [
      "eslint",
      "prettier --write"
    ]
  }
}

    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "prettier": "^3.3.3",
    "sass": "^1.79.2",
    "typescript": "latest"
  },
  "lint-staged": {
    "*.{js,ts,vue,scss}": [
      "eslint",
      "prettier --list-different"
    ]
  }
